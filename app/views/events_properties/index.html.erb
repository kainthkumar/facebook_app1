<h1>Listing events_properties</h1>

<table>
  <tr>
    <th>Event</th>
    <th>Name</th>
    <th>Value</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @events_properties.each do |events_property| %>
  <tr>
    <td><%= events_property.event_id %></td>
    <td><%= events_property.name %></td>
    <td><%= events_property.value %></td>
    <td><%= link_to 'Show', events_property %></td>
    <td><%= link_to 'Edit', edit_events_property_path(events_property) %></td>
    <td><%= link_to 'Destroy', events_property, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

